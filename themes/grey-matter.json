{
  "$schema": "vscode://schemas/color-theme",
  "name": "Syntax Snacks - Grey Matter",
  "type": "dark",
  "colors": {
    "editor.background": "#1e1e1e",
    "editor.foreground": "#d4d4d4",
    "editor.lineHighlightBackground": "#2a2a2a",
    "editor.selectionBackground": "#3a3a3a",
    "editorCursor.foreground": "#d4d4d4",
    "editorWhitespace.foreground": "#3a3a3a",
    "editorLineNumber.foreground": "#858585",
    "editorLineNumber.activeForeground": "#d4d4d4",
    "editorIndentGuide.background": "#3a3a3a",
    "editorIndentGuide.activeBackground": "#858585",
    "sideBar.background": "#181818",
    "sideBar.foreground": "#d4d4d4",
    "sideBarSectionHeader.background": "#1e1e1e",
    "statusBar.background": "#181818",
    "statusBar.foreground": "#d4d4d4",
    "activityBar.background": "#181818",
    "activityBar.foreground": "#d4d4d4",
    "titleBar.activeBackground": "#181818",
    "titleBar.activeForeground": "#d4d4d4"
  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#6a9955"
      }
    },
    {
      "name": "Keywords",
      "scope": ["keyword"],
      "settings": {
        "foreground": "#569cd6"
      }
    },
    {
      "name": "Strings",
      "scope": ["string"],
      "settings": {
        "foreground": "#ce9178"
      }
    },
    {
      "name": "Numbers",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#b5cea8"
      }
    },
    {
      "name": "Functions",
      "scope": ["entity.name.function"],
      "settings": {
        "foreground": "#dcdcaa"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable"],
      "settings": {
        "foreground": "#9cdcfe"
      }
    },
    {
      "name": "Types",
      "scope": ["entity.name.type", "storage.type"],
      "settings": {
        "foreground": "#4ec9b0"
      }
    },
    {
      "name": "Operators",
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#d4d4d4"
      }
    }
  ]
} 